{
  "manifest_version": 2,
  "name": "Chrome Socket API Server",
  "description": "listen & accept for socket",
  "version": "0.1",
  "app": {
    "background": {
      "scripts": [
        "lib/sha1.js",
        "lib/base64.js",
        "lib/parser.js",
        "server.js"]
    }
  },
  "permissions": [
    "experimental",
    {
      "socket": [ "tcp-listen" ]
    }
  ]
}
